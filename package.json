    "workbox-webpack-plugin": "4.3.1"
  },
  "nodemonConfig": {
    "ignore": [
      "tests/*",
      "docs/*",
      "src/*"
    ],
    "delay": "2500"
  },
  "//deb": {
    "afterInstall": "scripts/linux-package.after-install.sh",
    "afterRemove": "scripts/linux-package.after-remove.sh"
  }
}

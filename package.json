{
  "name": "notestory",
  "version": "1.0.1",
  "serverV": "0.1.0",
  "webV": "0.1.0",
  "adminV": "0.1.0",
  "private": true,
  "author": {
    "name": "atory",
    "email": "atory_cc@163.com"
  },
  "description": "simple note",
  "homepage": "http://wesy.club",
  "proxy": "http://localhost:3030",
  "main": "electron/main.js",
  "bin": "server/index.js",
  "scripts": {
    "electron:start": "cross-env PLANTFORM=electron node scripts/start.js",
    "electron:build": "cross-env PLANTFORM=electron PUBLIC_URL=./ npm run build",
    "electron:dev": "cp app_config/env-dev.json electron/env.json && ELECTRON_START_URL=http://localhost:5000 electron . --debug",
    "electron:windows:dev": "cross-env ELECTRON_START_URL=http://localhost:5000 electron . --debug",
    "server:dev": "cd server && DEBUG=app:* nodemon index.js -c config.json",
    "server-render:dev": "nodemon server-render/index.js",
    "server-admin:start": "cross-env PLANTFORM=web:admin node scripts/start.js",
    "server-admin:build": "cross-env PLANTFORM=web:admin node scripts/build.js",
    "start": "node scripts/start.js",
    "build": "node scripts/build.js",
    "update-schema": "node scripts/updateSchema.js",
    "relay": "relay-compiler --src ./src --schema ./src/data/schema.graphql",
    "test": "node scripts/test.js",
    "server:dist": "NODE_ENV=production node server/bin.js",
    "server-render:dist": "NODE_ENV=production node server-render/bin.js",
    "server-admin:dist": "NODE_ENV=production node server-admin/bin.js",
    "pack": "cp app_config/env-prod.json electron/env.json && electron-builder --dir",
    "dist": "rm -rf dist && cp app_config/env-prod.json electron/env.json && electron-builder",
    "windows:dist": "electron-builder",
    "dist:dev": "rm -rf dist && cp app_config/env-dev.json electron/env.json && electron-builder",
    "---": "---",
    "===": "===",
    "//server-render:build": "NODE_ENV=production pkg -c server-render/config.json -o server-render/note",
    "//server:build": "NODE_ENV=development pkg .",
    "//server:pkg": "pkg package.json -t node10 --out-path server && cp ./node_modules/opn/xdg-open ./dist/xdg-open",
    "electron": "npm run electron:build && electron . --debug",
    "//postinstall": "electron-builder install-app-deps"
  },
  "pkg": {
    "output": "server/server.bin",
    "targets": [
      "node10"
    ]
  },
  "//build.asar": "sharp link",
  "//asar": true,
  "build": {
    "appId": "club.wesy.notestory",
    "compression": "normal",
    "productName": "NoteStory",
    "artifactName": "${productName}-v${version}-${platform}-${arch}.${ext}",
    "directories": {
      "buildResources": "assets",
      "output": "dist"
    },
    "files": [
      "app_config/*",
      "electron/**/*",
      "assets/app-icon/png/*",
      "src/icon.png",
      "src/cover/*",
      "package.json"
    ],
    "mac": {
      "icon": "assets/app-icon/mac/icon.icns",
      "type": "distribution",
      "asarUnpack": "**/node_modules/sharp/**/*",
      "target": [
        "dmg"
      ],
      "category": "public.app-category.news"
    },
    "win": {
      "target": "nsis",
      "icon": "assets/app-icon/win/icon.ico"
    },
    "linux": {
      "asarUnpack": "**/node_modules/sharp/**/*",
      "target": [
        "deb",
        "appImage"
      ],
      "category": "Markdown, Note"
    }
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.d.ts"
    ],
    "resolver": "jest-pnp-resolver",
    "setupFiles": [
      "react-app-polyfill/jsdom"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,ts,tsx}",
      "<rootDir>/src/**/?(*.)(spec|test).{js,jsx,ts,tsx}"
    ],
    "testEnvironment": "jsdom",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|ts|tsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|ts|tsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|ts|tsx)$",
      "^.+\\.module\\.(css|sass|scss)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web",
      "^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "web.ts",
      "ts",
      "web.tsx",
      "tsx",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ],
    "watchPlugins": [
      "/home/ttang/note_story/node_modules/jest-watch-typeahead/filename.js",
      "/home/ttang/note_story/node_modules/jest-watch-typeahead/testname.js"
    ]
  },
  "dependencies": {
    "@atory/draft-js-markdown-converter": "^1.0.11",
    "@atory/draft-js-plugins-image": "^1.0.2",
    "@atory/draft-js-plugins-katex": "^1.0.2",
    "@atory/draft-js-plugins-markdown-shortcuts": "^1.0.12",
    "@atory/draft-js-static-toolbar": "^1.0.22",
    "@atory/markdown-it-katex": "^2.1.0",
    "@koa/cors": "^3.0.0",
    "@material-ui/core": "^4.1.1",
    "@material-ui/icons": "^4.2.0",
    "bcryptjs": "^2.4.3",
    "case-sensitive-paths-webpack-plugin": "2.2.0",
    "chart.js": "^2.8.0",
    "classnames": "^2.2.6",
    "clsx": "^1.0.4",
    "compare-versions": "^3.4.0",
    "config": "^3.1.0",
    "cos-nodejs-sdk-v5": "^2.5.9",
    "crypto-js": "^3.1.9-1",
    "debug": "^4.1.1",
    "dotenv": "^8.0.0",
    "dotenv-expand": "^5.1.0",
    "draft-js": "^0.10.5",
    "draft-js-image-plugin": "^2.0.6",
    "draft-js-plugins-editor": "^2.1.1",
    "draft-js-prism": "^1.0.6",
    "elastic-apm-node": "^2.11.6",
    "electron-log": "^3.0.6",
    "farce": "^0.2.8",
    "found": "^0.4.2",
    "found-relay": "^0.4.0",
    "fs-extra": "^8.0.1",
    "github-markdown-css": "^3.0.1",
    "graphql": "^14.3.1",
    "graphql-request": "^1.8.2",
    "history": "^4.9.0",
    "immutable": "^3.8.2",
    "jsonfile": "^5.0.0",
